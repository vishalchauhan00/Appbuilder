<?xml version="1.0" encoding="utf-8"?>
<item targetClass="GoogleSignIn" mockup="" visible="true" imagePath="assets/toolbox/ico_map.png" description="Here is the label description"
		helpLink_EN="" helpLink_JP="">
	<properties>
		<type name="Type" @stopCommit="true">
			<property path="name" input="spark.components.TextInput">
				<validations>
					<validation validator="UniqUINameValidator">
						<args>
							<arg property="currentSettings" value="{@currentSettings:}"/>
							<arg property="pageContainerDic" value="{@page:}"/>
							<arg property="uiPartDic" value="{@base:}"/>
							<arg property="tableViewCellDic" value="{@tableviewCell:}"/>
							<arg property="toolbarUiDic" value="{@toolbarUiDic:}"/>
						</args>
					</validation>
				</validations>
			</property>
			<property path="clientId" dataType="String" input="spark.components.TextInput" copyable="true" validator="RequiredValueValidator"/>
			<property path="secret" dataType="String" input="spark.components.TextInput" disable="true"/>
			<property path="theme" input="spark.components.DropDownList" init="Neutral" disable="true">
				<dataSource>
					<data name="Neutral"/>
					<data name="Light"/>
					<data name="Dark"/>
				</dataSource>
			</property>
			<property path="style" input="spark.components.DropDownList" init="Standard" copyable="true">
				<dataSource>
					<data name="Standard"/>
					<data name="Icon"/>
				</dataSource>
				<dependentActions>
					<when condition="compare" compareTarget="{@form[style]:value}" compareBy="EQ" compareWith="Standard" at="initForm, postCommit">
						<action target="{@form[text]:}" method="folded" value="false" caseOf="true"/>
						<action target="{@form[text]:}" method="folded" value="true" caseOf="false"/>
					</when>
				</dependentActions>
			</property>
			<property path="text" input="spark.components.DropDownList" init="Sign in" copyable="true">
				<dataSource>
					<data name="Sign in"/>
					<data name="Sign in with Google"/>
					<data name="Sign up with Google"/>
					<data name="Continue with Google"/>
				</dataSource>
			</property>
			<property path="shape" input="spark.components.DropDownList" init="Rectangular" copyable="true">
				<dataSource>
					<data name="Rectangular"/>
					<data name="Round"/>
				</dataSource>
			</property>			
			<property path="hidden" dataType="Boolean" input="spark.components.CheckBox" value="false" copyable="true"/>
		</type>
		<type name="Actions">
			<property path="actions.onAuthSuccess" input="LabelWithButton" />			
			<property path="actions.onAuthError" input="LabelWithButton" />
		</type>
		<type name="Geometry" @stopCommit="true">
			<property path="frame.x" dataType="Number" input="spark.components.NumericStepper" min="0" max="9999" copyable="true"/>
			<property path="frame.y" dataType="Number" input="spark.components.NumericStepper" min="0" max="9999" copyable="true"/>
			<property path="frame.width" dataType="Number" input="spark.components.NumericStepper" min="40" max="200" init="183" disable="true"/>
			<property path="frame.height" dataType="Number" input="spark.components.NumericStepper" min="40" max="40" init="40" disable="true"/>
		</type> 
	</properties>
</item>
