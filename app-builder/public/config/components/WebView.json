{
  "item": {
    "meta": {
      "targetClass": "WebView",
      "visible": true,
      "imagePath": "assets/toolbox/ico_www.png",
      "description": "Here is the label description"
    },
    "types": {
      "General": {
        "properties": {
          "name": {
            "input": "TextInput",
            "validators": [
              "UniqUINameValidator"
            ]
          },
          "filename.url": {
            "input": "TextInput",
            "dataType": "String",
            "copyable": true
          },
          "scalesPageToFit": {
            "input": "CheckBox",
            "dataType": "Boolean",
            "copyable": true
          },
          "hidden": {
            "input": "CheckBox",
            "dataType": "Boolean",
            "value": "false",
            "copyable": true
          }
        }
      },
      "Style": {
        "properties": {
          "borderColor": {
            "input": "ColorPicker",
            "dataType": "ColorDic",
            "copyable": true
          },
          "borderColor.alpha": {
            "input": "NumericStepper",
            "min": "0.0",
            "max": "1.0",
            "copyable": true
          },
          "borderWeight": {
            "input": "NumericStepper",
            "dataType": "Number",
            "init": "0",
            "min": "0",
            "max": "10",
            "copyable": true
          },
          "boxShadow": {
            "input": "CheckBox",
            "dataType": "Boolean",
            "value": "false",
            "disable": true,
            "dependentActions": [
              {
                "condition": {
                  "type": "compare",
                  "target": "{@form[boxShadow]:value}",
                  "compareBy": "EQ",
                  "compareWith": "1",
                  "at": [
                    "initForm",
                    " postCommit"
                  ]
                },
                "actions": [
                  {
                    "target": "{@form[boxShadowWidth]:}",
                    "method": "folded",
                    "value": "false",
                    "case": "true"
                  },
                  {
                    "target": "{@form[boxShadowWidth]:}",
                    "method": "folded",
                    "value": "true",
                    "case": "false"
                  },
                  {
                    "target": "{@form[boxShadowColor]:}",
                    "method": "folded",
                    "value": "false",
                    "case": "true"
                  },
                  {
                    "target": "{@form[boxShadowColor]:}",
                    "method": "folded",
                    "value": "true",
                    "case": "false"
                  }
                ]
              }
            ]
          },
          "boxShadowWidth": {
            "input": "NumericStepper",
            "min": "0",
            "max": "10",
            "disable": true
          },
          "boxShadowColor": {
            "input": "ColorPicker",
            "dataType": "ColorDic",
            "disable": true
          }
        }
      },
      "Actions": {
        "properties": {
          "actions.DidStartLoad": {
            "input": "LabelWithButton"
          },
          "actions.DidFailToLoad": {
            "input": "LabelWithButton"
          },
          "actions.DidFinishLoad": {
            "input": "LabelWithButton"
          }
        }
      },
      "Geometry": {
        "properties": {
          "frame.x": {
            "input": "NumericStepper",
            "dataType": "Number",
            "min": "0",
            "max": "9999",
            "copyable": true
          },
          "frame.y": {
            "input": "NumericStepper",
            "dataType": "Number",
            "min": "0",
            "max": "9999",
            "copyable": true
          },
          "frame.width": {
            "input": "NumericStepper",
            "dataType": "Number",
            "init": "100",
            "min": "100",
            "max": "9999",
            "copyable": true
          },
          "frame.height": {
            "input": "NumericStepper",
            "dataType": "Number",
            "init": "100",
            "min": "40",
            "max": "9999",
            "copyable": true
          }
        }
      }
    }
  }
}