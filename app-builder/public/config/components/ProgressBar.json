{
  "item": {
    "meta": {
      "targetClass": "ProgressBar",
      "visible": true,
      "imagePath": "assets/toolbox/ico_progress.png",
      "description": "Here is the label description"
    },
    "types": {
      "General": {
        "properties": {
          "name": {
            "input": "TextInput",
            "validators": [
              "UniqUINameValidator"
            ]
          },
          "fieldname": {
            "input": "DropDownList123",
            "dataSource": [],
            "dependentActions": [
              {
                "condition": {
                  "type": "compare",
                  "target": "@dbTable:",
                  "compareBy": "IS_NULL_OR_EMPTY",
                  "at": [
                    "initForm"
                  ]
                },
                "actions": [
                  {
                    "target": "{@form[fieldname]:}",
                    "method": "folded",
                    "value": "true",
                    "case": "true"
                  },
                  {
                    "target": "{@form[fieldname]:}",
                    "method": "folded",
                    "value": "false",
                    "case": "false"
                  },
                  {
                    "target": "{@form[fieldname]:}",
                    "method": "setOptions",
                    "value": "@dbTable:fieldswithBracket",
                    "case": "false"
                  }
                ]
              },
              {
                "condition": {
                  "type": "compare",
                  "target": "{@form[fieldname]:value}",
                  "compareBy": "IS_NULL_OR_EMPTY",
                  "at": [
                    "postCommit"
                  ]
                },
                "actions": [
                  {
                    "target": "{@form[progress]:}",
                    "method": "setValue",
                    "value": "{@form[fieldname]:value}",
                    "case": "false"
                  }
                ]
              }
            ]
          },
          "progress": {
            "input": "NumericStepper",
            "dataType": "Number",
            "init": "0.1",
            "min": "0",
            "max": "1",
            "copyable": true
          },
          "style": {
            "input": "DropDownList",
            "dataType": "array",
            "init": "Bar",
            "copyable": true,
            "dataSource": [
              {
                "name": "Bar"
              },
              {
                "name": "Circle"
              }
            ],
            "dependentActions": [
              {
                "condition": {
                  "type": "compare",
                  "target": "{@form[style]:value}",
                  "compareBy": "EQ",
                  "compareWith": "Bar",
                  "at": [
                    "initForm",
                    " postCommit"
                  ]
                },
                "actions": [
                  {
                    "target": "{@form[arcRadius]:}",
                    "method": "folded",
                    "value": "true",
                    "case": "true"
                  },
                  {
                    "target": "{@form[borderColor]:}",
                    "method": "hide",
                    "value": "true",
                    "case": "true"
                  },
                  {
                    "target": "{@form[borderColor.alpha]:}",
                    "method": "hide",
                    "value": "true",
                    "case": "true"
                  },
                  {
                    "target": "{@form[borderWeight]:}",
                    "method": "hide",
                    "value": "true",
                    "case": "true"
                  },
                  {
                    "target": "{@form[cornerRadius]:}",
                    "method": "hide",
                    "value": "true",
                    "case": "true"
                  },
                  {
                    "target": "{@form[frame.width]:}",
                    "method": "hide",
                    "value": "true",
                    "case": "true"
                  },
                  {
                    "target": "{@form[frame.height]:}",
                    "method": "hide",
                    "value": "true",
                    "case": "true"
                  }
                ]
              },
              {
                "condition": {
                  "type": "compare",
                  "target": "{@form[style]:value}",
                  "compareBy": "EQ",
                  "compareWith": "Circle",
                  "at": [
                    "initForm",
                    " postCommit"
                  ]
                },
                "actions": [
                  {
                    "target": "{@form[arcRadius]:}",
                    "method": "folded",
                    "value": "false",
                    "case": "true"
                  },
                  {
                    "target": "{@form[borderColor]:}",
                    "method": "hide",
                    "value": "false",
                    "case": "true"
                  },
                  {
                    "target": "{@form[borderColor.alpha]:}",
                    "method": "hide",
                    "value": "false",
                    "case": "true"
                  },
                  {
                    "target": "{@form[borderWeight]:}",
                    "method": "hide",
                    "value": "false",
                    "case": "true"
                  },
                  {
                    "target": "{@form[cornerRadius]:}",
                    "method": "hide",
                    "value": "false",
                    "case": "true"
                  },
                  {
                    "target": "{@form[frame.width]:}",
                    "method": "hide",
                    "value": "false",
                    "case": "true"
                  },
                  {
                    "target": "{@form[frame.height]:}",
                    "method": "hide",
                    "value": "false",
                    "case": "true"
                  }
                ]
              }
            ]
          },
          "arcRadius": {
            "input": "NumericStepper",
            "min": "10",
            "max": "100",
            "copyable": true
          },
          "hidden": {
            "input": "CheckBox",
            "dataType": "Boolean",
            "value": "false",
            "copyable": true
          }
        }
      },
      "Style": {
        "properties": {
          "backgroundColor": {
            "input": "ColorPicker",
            "dataType": "ColorDic",
            "init": "0",
            "copyable": true
          },
          "fillColor": {
            "input": "ColorPicker",
            "dataType": "ColorDic",
            "init": "0",
            "copyable": true
          },
          "borderColor": {
            "input": "ColorPicker",
            "dataType": "ColorDic",
            "copyable": true
          },
          "borderColor.alpha": {
            "input": "NumericStepper",
            "min": "0.0",
            "max": "1.0",
            "disable": true
          },
          "borderWeight": {
            "input": "NumericStepper",
            "dataType": "Number",
            "init": "0",
            "min": "0",
            "max": "5",
            "copyable": true
          }
        }
      },
      "Geometry": {
        "properties": {
          "frame.x": {
            "input": "NumericStepper",
            "dataType": "Number",
            "min": "0",
            "max": "10000",
            "copyable": true
          },
          "frame.y": {
            "input": "NumericStepper",
            "dataType": "Number",
            "min": "0",
            "max": "10000",
            "copyable": true
          },
          "frame.width": {
            "input": "NumericStepper",
            "dataType": "Number",
            "init": "200",
            "min": "120",
            "max": "10000",
            "copyable": true
          },
          "frame.height": {
            "input": "NumericStepper",
            "dataType": "Number",
            "init": "15",
            "min": "8",
            "max": "56",
            "copyable": true
          }
        }
      }
    }
  }
}