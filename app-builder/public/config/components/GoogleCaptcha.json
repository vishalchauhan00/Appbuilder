{
  "item": {
    "meta": {
      "targetClass": "GoogleCaptcha",
      "visible": true,
      "imagePath": "assets/toolbox/ico_map.png",
      "description": "Here is the label description"
    },
    "types": {
      "General": {
        "properties": {
          "name": {
            "input": "TextInput",
            "validators": [
              "UniqUINameValidator"
            ]
          },
          "sitekey": {
            "input": "TextInput",
            "dataType": "String",
            "copyable": true
          },
          "type": {
            "input": "DropDownList",
            "init": "Checkbox",
            "copyable": true,
            "dataSource": [
              {
                "name": "Checkbox"
              },
              {
                "name": "Invisible"
              }
            ],
            "dependentActions": [
              {
                "condition": {
                  "type": "compare",
                  "target": "{@form[type]:value}",
                  "compareBy": "EQ",
                  "compareWith": "Checkbox",
                  "at": [
                    "initForm",
                    " postCommit"
                  ]
                },
                "actions": [
                  {
                    "target": "{@form[theme]:}",
                    "method": "folded",
                    "value": "false",
                    "case": "true"
                  },
                  {
                    "target": "{@form[size]:}",
                    "method": "folded",
                    "value": "false",
                    "case": "true"
                  },
                  {
                    "target": "{@form[badge]:}",
                    "method": "folded",
                    "value": "true",
                    "case": "true"
                  },
                  {
                    "target": "{@form[theme]:}",
                    "method": "folded",
                    "value": "true",
                    "case": "false"
                  },
                  {
                    "target": "{@form[size]:}",
                    "method": "folded",
                    "value": "true",
                    "case": "false"
                  },
                  {
                    "target": "{@form[badge]:}",
                    "method": "folded",
                    "value": "false",
                    "case": "false"
                  }
                ]
              },
              {
                "condition": {
                  "type": "compare",
                  "target": "{@form[type]:value}",
                  "compareBy": "EQ",
                  "compareWith": "Invisible",
                  "at": [
                    "initForm",
                    " postCommit"
                  ]
                },
                "actions": [
                  {
                    "target": "{@form[theme]:}",
                    "method": "folded",
                    "value": "true",
                    "case": "true"
                  },
                  {
                    "target": "{@form[size]:}",
                    "method": "folded",
                    "value": "true",
                    "case": "true"
                  },
                  {
                    "target": "{@form[badge]:}",
                    "method": "folded",
                    "value": "false",
                    "case": "true"
                  },
                  {
                    "target": "{@form[theme]:}",
                    "method": "folded",
                    "value": "false",
                    "case": "false"
                  },
                  {
                    "target": "{@form[size]:}",
                    "method": "folded",
                    "value": "false",
                    "case": "false"
                  },
                  {
                    "target": "{@form[badge]:}",
                    "method": "folded",
                    "value": "true",
                    "case": "false"
                  }
                ]
              }
            ]
          },
          "theme": {
            "input": "DropDownList",
            "init": "light",
            "copyable": true,
            "dataSource": [
              {
                "name": "light"
              },
              {
                "name": "dark"
              }
            ]
          },
          "size": {
            "input": "DropDownList",
            "init": "normal",
            "copyable": true,
            "dataSource": [
              {
                "name": "normal"
              },
              {
                "name": "compact"
              }
            ]
          },
          "badge": {
            "input": "DropDownList",
            "init": "bottomright",
            "copyable": true,
            "dataSource": [
              {
                "name": "bottomright"
              },
              {
                "name": "bottomleft"
              }
            ]
          },
          "hidden": {
            "input": "CheckBox",
            "dataType": "Boolean",
            "value": "false",
            "copyable": true
          }
        }
      },
      "Actions": {
        "properties": {
          "actions.onSuccess": {
            "input": "LabelWithButton"
          },
          "actions.onError": {
            "input": "LabelWithButton"
          }
        }
      },
      "Geometry": {
        "properties": {
          "frame.x": {
            "input": "NumericStepper",
            "dataType": "Number",
            "min": "0",
            "max": "9999",
            "copyable": true
          },
          "frame.y": {
            "input": "NumericStepper",
            "dataType": "Number",
            "min": "0",
            "max": "9999",
            "copyable": true
          },
          "frame.width": {
            "input": "NumericStepper",
            "dataType": "Number",
            "init": "183",
            "min": "40",
            "max": "200",
            "disable": true
          },
          "frame.height": {
            "input": "NumericStepper",
            "dataType": "Number",
            "init": "40",
            "min": "40",
            "max": "40",
            "disable": true
          }
        }
      }
    }
  }
}