          },
          "padding.right": {
            "input": "NumericStepper",
            "dataType": "Number",
            "init": "2",
            "min": "0",
            "max": "100",
            "copyable": true
          },
          "padding.bottom": {
            "input": "NumericStepper",
            "dataType": "Number",
            "init": "2",
            "min": "0",
            "max": "100",
            "copyable": true
          }
        }
