{
  "item": {
    "meta": {
      "targetClass": "SearchBar",
      "visible": true,
      "imagePath": "assets/toolbox/ico_searchbar.png",
      "description": "Here is the SearchBar description"
    },
    "types": {
      "General": {
        "properties": {
          "name": {
            "input": "TextInput",
            "validators": [
              "UniqUINameValidator"
            ]
          },
          "fieldname": {
            "input": "DropDownList123",
            "dataSource": [],
            "dependentActions": [
              {
                "condition": {
                  "type": "compare",
                  "target": "@dbTable:",
                  "compareBy": "IS_NULL_OR_EMPTY",
                  "at": [
                    "initForm"
                  ]
                },
                "actions": [
                  {
                    "target": "{@form[fieldname]:}",
                    "method": "folded",
                    "value": "true",
                    "case": "true"
                  },
                  {
                    "target": "{@form[fieldname]:}",
                    "method": "folded",
                    "value": "false",
                    "case": "false"
                  },
                  {
                    "target": "{@form[fieldname]:}",
                    "method": "setOptions",
                    "value": "@dbTable:fieldswithBracket",
                    "case": "false"
                  }
                ]
              },
              {
                "condition": {
                  "type": "compare",
                  "target": "{@form[fieldname]:value}",
                  "compareBy": "IS_NULL_OR_EMPTY",
                  "at": [
                    "postCommit"
                  ]
                },
                "actions": [
                  {
                    "target": "{@form[text]:}",
                    "method": "setValue",
                    "value": "{@form[fieldname]:value}",
                    "case": "false"
                  }
                ]
              }
            ]
          },
          "text": {
            "input": "TextInput",
            "dataType": "String",
            "copyable": true
          },
          "placeholder": {
            "input": "TextInput",
            "dataType": "String",
            "copyable": true
          },
          "barStyle": {
            "input": "DropDownList",
            "dataType": "array",
            "init": "blue",
            "copyable": true,
            "dataSource": [
              {
                "name": "blue"
              },
              {
                "name": "black"
              },
              {
                "name": "silver"
              },
              {
                "name": "white"
              }
            ]
          },
          "hidden": {
            "input": "CheckBox",
            "dataType": "Boolean",
            "value": "false",
            "copyable": true
          }
        }
      },
      "Style": {
        "properties": {
          "backgroundColor": {
            "input": "ColorPicker",
            "dataType": "ColorDic",
            "copyable": true
          },
          "backgroundColor.alpha": {
            "input": "NumericStepper",
            "min": "0.0",
            "max": "1.0",
            "copyable": true
          },
          "borderWeight": {
            "input": "NumericStepper",
            "dataType": "Number",
            "min": "0",
            "max": "10",
            "copyable": true
          },
          "borderColor": {
            "input": "ColorPicker",
            "dataType": "ColorDic",
            "copyable": true
          },
          "borderColor.alpha": {
            "input": "NumericStepper",
            "min": "0.0",
            "max": "1.0",
            "copyable": true
          }
        }
      },
      "font": {
        "properties": {
          "font.fontName": {
            "input": "DropDownList",
            "dataType": "array",
            "init": "system",
            "copyable": true,
            "dataSource": []
          },
          "font.fontSize": {
            "input": "NumericStepper",
            "dataType": "Number",
            "init": "12",
            "min": "6",
            "max": "300",
            "copyable": true
          },
          "font.textColor": {
            "input": "ColorPicker",
            "dataType": "ColorDic",
            "copyable": true
          },
          "font.textAlignment": {
            "input": "RadioButton",
            "dataType": "custom",
            "init": "left",
            "copyable": true,
            "dataSource": [
              {
                "name": "left"
              },
              {
                "name": "center"
              },
              {
                "name": "right"
              }
            ]
          },
          "verticalAlignment": {
            "input": "RadioButton",
            "dataType": "custom",
            "init": "middle",
            "copyable": true,
            "dataSource": [
              {
                "name": "top"
              },
              {
                "name": "middle"
              },
              {
                "name": "bottom"
              }
            ]
          },
          "placeholderColor": {
            "input": "ColorPicker",
            "dataType": "ColorDic",
            "disable": true
          }
        }
      },
      "TextPadding": {
        "properties": {
          "padding.top": {
            "input": "NumericStepper",
            "dataType": "Number",
            "init": "2",
            "min": "0",
            "max": "100",
            "copyable": true
          },
          "padding.left": {
            "input": "NumericStepper",
            "dataType": "Number",
            "init": "2",
            "min": "0",
            "max": "100",
            "copyable": true
          },
          "padding.right": {
            "input": "NumericStepper",
            "dataType": "Number",
            "init": "2",
            "min": "0",
            "max": "100",
            "copyable": true
          },
          "padding.bottom": {
            "input": "NumericStepper",
            "dataType": "Number",
            "init": "2",
            "min": "0",
            "max": "100",
            "copyable": true
          }
        }
      },
      "Actions": {
        "properties": {
          "actions.textBeginEditing": {
            "input": "LabelWithButton"
          },
          "actions.texEndEditing": {
            "input": "LabelWithButton"
          },
          "actions.textDidChanged": {
            "input": "LabelWithButton"
          },
          "actions.CancelClicked": {
            "input": "LabelWithButton"
          }
        }
      },
      "Geometry": {
        "properties": {
          "frame.x": {
            "input": "NumericStepper",
            "dataType": "Number",
            "min": "0",
            "max": "10000",
            "copyable": true
          },
          "frame.y": {
            "input": "NumericStepper",
            "dataType": "Number",
            "min": "0",
            "max": "10000",
            "copyable": true
          },
          "frame.width": {
            "input": "NumericStepper",
            "dataType": "Number",
            "init": "260",
            "min": "50",
            "max": "10000",
            "copyable": true
          },
          "frame.height": {
            "input": "NumericStepper",
            "dataType": "Number",
            "init": "30",
            "min": "30",
            "max": "100",
            "copyable": true
          }
        }
      }
    }
  }
}