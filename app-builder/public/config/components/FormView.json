              {
                "condition": {
                  "type": "compare",
                  "target": "{@form[boxShadow]:value}",
                  "compareBy": "EQ",
                  "compareWith": "1",
                  "at": [
                    "initForm",
                    " postCommit"
                  ]
                },
                "actions": [
                  {
                    "target": "{@form[boxShadowWidth]:}",
                    "method": "folded",
                    "value": "false",
                    "case": "true"
                  },
                  {
                    "target": "{@form[boxShadowWidth]:}",
                    "method": "folded",
                    "value": "true",
                    "case": "false"
