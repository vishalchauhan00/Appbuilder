              {
                "name": "DB"
              }
            ],
            "dependentActions": [
              {
                "condition": {
                  "type": "compare",
                  "target": "{@form[type]:value}",
                  "compareBy": "EQ",
                  "compareWith": "Fixed",
                  "at": [
                    "initForm",
                    " postCommit"
                  ]
                },
                "actions": [
                  {
                    "target": "{@form[dataarray]:}",
                    "method": "folded",
                    "value": "false",
                    "case": "true"
                  },
                  {
                    "target": "{@form[dataarray[*].text]:}",
                    "method": "folded",
                    "value": "false",
                    "case": "true"
                  },
                  {
                    "target": "{@form[dataarray[*].fieldvalue]:}",
                    "method": "folded",
                    "value": "false",
                    "case": "true"
