          "backgroundColor.alpha": {
            "input": "NumericStepper",
            "min": "0.0",
            "max": "1.0",
            "copyable": true
          },
          "borderColor": {
            "input": "ColorPicker",
            "dataType": "ColorDic",
            "copyable": true
