            "dataType": "Number",
            "init": "2",
            "min": "0",
            "max": "100",
            "copyable": true
          }
        }
      },
      "Geometry": {
        "properties": {
          "frame.x": {
            "input": "NumericStepper",
            "dataType": "Number",
            "min": "0",
            "max": "10000",
            "copyable": true
          },
          "frame.y": {
            "input": "NumericStepper",
            "dataType": "Number",
            "min": "0",
            "max": "10000",
            "copyable": true
