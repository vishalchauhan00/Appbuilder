                    "target": "{@form[dataarray[*].text]:}",
                    "method": "folded",
                    "value": "true",
                    "case": "true"
                  },
                  {
                    "target": "{@form[dataarray[*].fieldvalue]:}",
                    "method": "folded",
                    "value": "true",
                    "case": "true"
                  },
                  {
                    "target": "{@form[SelectedValues]:}",
                    "method": "folded",
                    "value": "true",
                    "case": "true"
                  }
                ]
              }
            ]
          }
        }
      },
      "Items": {
        "properties": {
          "dataarray": {
            "input": "ListWithMultipleSelection",
            "copyable": true,
            "dependentActions": [
              {
                "condition": {
                  "at": [
                    "initForm",
                    " postIndexChange"
                  ]
                },
                "actions": [
                  {
                    "target": "{@form[dataarray[*].text]:}",
                    "method": "setValue",
                    "value": "{@base:dataarray[@form[dataarray]:selectedIndex].text}"
                  },
                  {
                    "target": "{@form[dataarray[*].text]:}",
                    "method": "setPath",
                    "value": "{\"dataarray[\" + @form[dataarray]:selectedIndex + \"].text\"}"
                  },
                  {
                    "target": "{@form[dataarray[*].fieldvalue]:}",
                    "method": "setValue",
                    "value": "{@base:dataarray[@form[dataarray]:selectedIndex].fieldvalue}"
                  },
                  {
                    "target": "{@form[dataarray[*].fieldvalue]:}",
                    "method": "setPath",
                    "value": "{\"dataarray[\" + @form[dataarray]:selectedIndex + \"].fieldvalue\"}"
                  }
                ]
              }
            ]
          },
          "dataarray[0].text": {
            "input": "TextInput",
            "copyable": true
          },
          "dataarray[0].fieldvalue": {
            "input": "TextInput",
            "dataType": "String",
            "copyable": true
          },
          "ServiceName": {
            "input": "DropDownList",
            "dataSource": [],
            "dependentActions": [
              {
                "condition": {
                  "at": [
                    "initForm",
                    "postCommit"
                  ]
                },
                "actions": [
                  {
                    "target": "{@form[tablename]:}",
                    "method": "setOptions",
                    "value": "{@project:getDbTableDicsByServiceName(@form[ServiceName]:value)}",
                    "case": "true"
                  },
                  {
                    "target": "{@form[tablename]:}",
                    "method": "setProperty",
                    "value": "-1",
                    "case": "true"
                  }
                ]
              }
            ]
          },
          "tablename": {
            "input": "DropDownList",
            "dataSource": [],
            "dependentActions": [
              {
                "condition": {
                  "type": "compare",
                  "target": "{@form[tablename]:value}",
                  "compareBy": "NE",
                  "compareWith": "",
                  "at": [
                    "initForm",
                    " postCommit"
                  ]
                },
                "actions": [
                  {
                    "target": "{@form[fieldname]:}",
                    "method": "setOptions",
                    "value": "{@project:getBracedDbFields(@form[ServiceName]:value, @form[tablename]:value)}",
                    "case": "true"
                  }
                ]
              }
            ]
          },
          "where": {
            "input": "TextInput"
          },
          "sort": {
            "input": "TextInput"
          },
          "fieldname": {
            "input": "DropDownList",
            "dependentActions": [
              {
                "condition": {
                  "type": "compare",
                  "target": "{@form[fieldname]:value}",
                  "compareBy": "NE",
                  "compareWith": "",
                  "at": [
                    "postCommit",
                    " postIndexChange"
                  ]
                },
                "actions": [
                  {
                    "target": "{@form[displayText]:}",
                    "method": "setValue",
                    "value": "{@form[fieldname]:value}",
                    "case": "true"
                  }
                ]
              }
            ]
          },
          "displayText": {
            "input": "TextInput"
          },
          "initialValue": {
            "input": "TextInput"
          }
        }
      },
      "Font": {
        "properties": {
          "font.textColor": {
            "input": "ColorPicker",
            "dataType": "ColorDic",
            "copyable": true
          },
          "font.textAlignment": {
            "input": "RadioButton",
            "dataType": "custom",
            "init": "left",
            "copyable": true,
            "dataSource": [
              {
                "name": "left"
              },
              {
                "name": "center"
              },
              {
                "name": "right"
              }
            ]
          },
          "font.fontSize": {
            "input": "NumericStepper",
            "dataType": "Number",
            "init": "14",
            "min": "6",
            "max": "100",
            "copyable": true
          },
          "verticalAlignment": {
            "input": "RadioButton",
            "dataType": "custom",
            "init": "middle",
            "copyable": true,
            "dataSource": [
              {
                "name": "top"
              },
              {
                "name": "middle"
              },
              {
                "name": "bottom"
              }
            ]
          }
        }
      },
      "TextPadding": {
        "properties": {
          "padding.top": {
            "input": "NumericStepper",
            "dataType": "Number",
