{
  "item": {
    "meta": {
      "targetClass": "GoogleMap",
      "visible": true,
      "imagePath": "assets/toolbox/ico_map.png",
      "description": "Here is the label description"
    },
    "types": {
      "General": {
        "properties": {
          "name": {
            "input": "TextInput",
            "validators": [
              "UniqUINameValidator"
            ]
          },
          "initialPosition": {
            "input": "TextInput",
            "dataType": "String",
            "copyable": true
          },
          "zoom": {
            "input": "NumericStepper",
            "dataType": "Number",
            "init": "8",
            "min": "0",
            "max": "21",
            "copyable": true
          },
          "GPS": {
            "input": "CheckBox",
            "dataType": "Boolean",
            "value": "0",
            "copyable": true
          },
          "hidden": {
            "input": "CheckBox",
            "dataType": "Boolean",
            "value": "false",
            "copyable": true
          }
        }
      },
      "Actions": {
        "properties": {
          "actions.regionWillChange": {
            "input": "LabelWithButton"
          },
          "actions.regionDidChange": {
            "input": "LabelWithButton"
          },
          "actions.WillStartLoadingMap": {
            "input": "LabelWithButton"
          },
          "actions.DidFinishLoadingMap": {
            "input": "LabelWithButton"
          },
          "actions.DidFailLoadingMap": {
            "input": "LabelWithButton"
          }
        }
      },
      "Geometry": {
        "properties": {
          "frame.x": {
            "input": "NumericStepper",
            "dataType": "Number",
            "min": "0",
            "max": "9999",
            "copyable": true
          },
          "frame.y": {
            "input": "NumericStepper",
            "dataType": "Number",
            "min": "0",
            "max": "9999",
            "copyable": true
          },
          "frame.width": {
            "input": "NumericStepper",
            "dataType": "Number",
            "init": "200",
            "min": "100",
            "max": "9999",
            "copyable": true
          },
          "frame.height": {
            "input": "NumericStepper",
            "dataType": "Number",
            "init": "200",
            "min": "100",
            "max": "9999",
            "copyable": true
          }
        }
      }
    }
  }
}