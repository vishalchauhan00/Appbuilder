<?xml version="1.0" encoding="utf-8"?>
<item targetClass="NestedList" mockup="com.mobilous.builder.client.pageeditor.editor.mockup.TileList.TileListView" visible="true" visibleInTableView="true" imagePath="assets/toolbox/ico_tilelist.png" description="Here is the label description"
		>
	<properties>
		<type name="Type" @stopCommit="true">
			<property path="name" input="spark.components.TextInput">
				<validations>
					<validation validator="UniqUINameValidator">
						<args>
							<arg property="currentSettings" value="{@currentSettings:}"/>
							<arg property="pageContainerDic" value="{@page:}"/>
							<arg property="uiPartDic" value="{@base:}"/>
							<arg property="tableViewCellDic" value="{@tableviewCell:}"/>
							<arg property="toolbarUiDic" value="{@toolbarUiDic:}"/>
						</args>
					</validation>
				</validations>
			</property>
			<property path="hidden" dataType="Boolean" input="spark.components.CheckBox" value="false" copyable="true"/>
		</type>
		<type name="Style" @stopCommit="true">
			<property path="showPagination" dataType="Boolean" input="spark.components.CheckBox" value="false" disable="true" >
				<dependentActions>
					<when condition="compare" compareTarget="{@form[showPagination]:value}" compareBy="EQ" compareWith="1" at="initForm, postCommit">
						<action target="{@form[paginationColor]:}" method="folded" value="false" caseOf="true"/>
						<action target="{@form[paginationColor]:}" method="folded" value="true" caseOf="false"/>						
					</when>
				</dependentActions>
			</property>
			<property path="paginationColor" dataType="ColorDic" input="mx.controls.ColorPicker" disable="true"/>
			<property path="backgroundColor" dataType="ColorDic" input="mx.controls.ColorPicker" copyable="true"/>
			<property path="backgroundColor.alpha" input="spark.components.NumericStepper" min="0.0" max="1.0" inc="0.1" copyable="true"/>
			<property path="borderColor" dataType="ColorDic" input="mx.controls.ColorPicker" copyable="true"/>
			<property path="borderColor.alpha" input="spark.components.NumericStepper" min="0.0" max="1.0" inc="0.1" copyable="true"/>
			<property path="borderWeight" dataType="Number" input="spark.components.NumericStepper" min="0" max="10" inc="1" init="0" copyable="true"/>
			<property path="cornerRadius" dataType="Number" input="spark.components.NumericStepper" min="0" max="25" inc="1" init="0" copyable="true"/>
			<property path="boxShadow" dataType="Boolean" input="spark.components.CheckBox" value="false" copyable="true">
				<dependentActions>
					<when condition="compare" compareTarget="{@form[boxShadow]:value}" compareBy="EQ" compareWith="1" at="initForm, postCommit">
						<action target="{@form[boxShadowWidth]:}" method="folded" value="false" caseOf="true"/>
						<action target="{@form[boxShadowWidth]:}" method="folded" value="true" caseOf="false"/>
						<action target="{@form[boxShadowColor]:}" method="folded" value="false" caseOf="true"/>
						<action target="{@form[boxShadowColor]:}" method="folded" value="true" caseOf="false"/>						
					</when>
				</dependentActions>
			</property>
			<property path="boxShadowWidth" input="spark.components.NumericStepper" min="0" max="10" inc="1" copyable="true"/>
			<property path="boxShadowColor" dataType="ColorDic" input="mx.controls.ColorPicker" copyable="true"/>
		</type>	
		
		<type name="MainCell">
			<property path="mainCellDef.ServiceName" input="spark.components.DropDownList">
				<dataSource source="@project:getServices()" labelField="label" valueField="value"/>
				<dependentActions>
					<when at="initForm,postCommit" stopCommit="true">
						<action target="{@form[mainCellDef.tablename]:}" method="setOptions" value="{@project:getDbTableDicsByServiceName(@form[mainCellDef.ServiceName]:value)}" labelField="tablename" valueField="tablename"/>
						<action target="{@form[mainCellDef.tablename]:}"  method="setProperty" property="selectedIndex" value="-1"/> 
						<action target="{@form[mainCellDef.fieldname]:}" method="setOptions" value="{@project:getBracedDbFields(@form[mainCellDef.ServiceName]:value, @form[mainCellDef.tablename]:value)}" labelField="fieldname" valueField="fieldname"/>
					</when>
				</dependentActions>
			</property>
			<property path="mainCellDef.tablename" input="spark.components.DropDownList">
				<dataSource source="@project:getDbTableDicsByServiceName(@form[mainCellDef.ServiceName]:value)" labelField="tablename" valueField="tablename"/>
				<dependentActions>
					<when at="initForm, postCommit" condition="compare" compareTarget="{@form[mainCellDef.tablename]:value}" compareBy="NE" compareWith="">						
						<action target="{@form[mainCellDef.fieldname]:}" method="setOptions" value="{@project:getBracedDbFields(@form[mainCellDef.ServiceName]:value, @form[mainCellDef.tablename]:value)}" labelField="fieldname" valueField="fieldname" caseOf="true"/>
					</when>
				</dependentActions>
			</property>
			<property path="mainCellDef.where" input="spark.components.TextInput" validator="whereClauseValidator"/>
			<property path="mainCellDef.sort" input="spark.components.TextInput"/>
			<property path="mainCellDef.limit" input="spark.components.NumericStepper" min="0" max="50" inc="1" init="0"/>
			<property path="mainCellDef.height" dataType="Number" input="spark.components.NumericStepper" min="10" max="10000" init="50" copyable="true"/>
			<property path="mainCellDef.Fields" input="LabelWithButton" popup="MainCellEditor" source="NestedList">
				<dependentActions>
					<when at="preCommit" stopCommit="true">
						<action target="{@form[mainCellDef.Fields]:}" method="{@base:notifyChange()}"/>
					</when>
				</dependentActions>
			</property>
			<property path="mainCellDef.isCollapsible" input="spark.components.CheckBox" init="0" disable="true" >
				<dependentActions>
					<when condition="compare" compareTarget="{@form[mainCellDef.isCollapsible]:value}" compareBy="EQ" compareWith="0" at="initForm, postCommit">
						<action target="{@form[mainCellDef.iconPosition]:}" method="folded" value="true" caseOf="true" />
						<action target="{@form[mainCellDef.iconPosition]:}" method="folded" value="false" caseOf="false" />
						<action target="{@form[mainCellDef.collapseIcon]:}" method="folded" value="true" caseOf="true" />
						<action target="{@form[mainCellDef.collapseIcon]:}" method="folded" value="false" caseOf="false" />
						<action target="{@form[mainCellDef.expandIcon]:}" method="folded" value="true" caseOf="true" />
						<action target="{@form[mainCellDef.expandIcon]:}" method="folded" value="false" caseOf="false" />
					</when>
				</dependentActions>
			</property>
			<property path="mainCellDef.iconPosition" dataType="array" input="spark.components.DropDownList" init="left" disable="true">
				<dataSource>
					<data name="left"/>
					<data name="right"/>
				</dataSource>
			</property>		
			<property path="mainCellDef.collapseIcon" input="TextInputWithButton" disable="true"/>
			<property path="mainCellDef.expandIcon" input="TextInputWithButton" disable="true"/>
			<property path="mainCellDef.backgroundColor" dataType="ColorDic" input="mx.controls.ColorPicker" copyable="true"/>
			<property path="mainCellDef.backgroundColor.alpha" input="spark.components.NumericStepper" min="0.0" max="1.0" inc="0.1" copyable="true"/>
			<property path="mainCellDef.borderColor" dataType="ColorDic" input="mx.controls.ColorPicker" disable="true"/>
			<property path="mainCellDef.borderColor.alpha" input="spark.components.NumericStepper" min="0.0" max="1.0" inc="0.1" disable="true"/>
			<property path="mainCellDef.borderWeight" dataType="Number" input="spark.components.NumericStepper" min="0" max="10" inc="1" init="0" disable="true"/>
			<property path="mainCellDef.cornerRadius" dataType="Number" input="spark.components.NumericStepper" min="0" max="25" inc="1" init="0" disable="true"/>
		</type>	
		<type name="SubCell">
			<property path="subCellDef.ServiceName" input="spark.components.DropDownList">
				<dataSource source="@project:getServices()" labelField="label" valueField="value"/>
				<dependentActions>
					<when at="initForm,postCommit" stopCommit="true">
						<action target="{@form[subCellDef.tablename]:}" method="setOptions" value="{@project:getDbTableDicsByServiceName(@form[subCellDef.ServiceName]:value)}" labelField="tablename" valueField="tablename"/>
						<action target="{@form[subCellDef.tablename]:}"  method="setProperty" property="selectedIndex" value="-1"/> 
						<action target="{@form[subCellDef.fieldname]:}" method="setOptions" value="{@project:getBracedDbFields(@form[subCellDef.ServiceName]:value, @form[subCellDef.tablename]:value)}" labelField="fieldname" valueField="fieldname"/>
					</when>
				</dependentActions>
			</property>
			<property path="subCellDef.tablename" input="spark.components.DropDownList">
				<dataSource source="@project:getDbTableDicsByServiceName(@form[subCellDef.ServiceName]:value)" labelField="tablename" valueField="tablename"/>
				<dependentActions>
					<when at="initForm, postCommit" condition="compare" compareTarget="{@form[subCellDef.tablename]:value}" compareBy="NE" compareWith="">						
						<action target="{@form[subCellDef.fieldname]:}" method="setOptions" value="{@project:getBracedDbFields(@form[subCellDef.ServiceName]:value, @form[subCellDef.tablename]:value)}" labelField="fieldname" valueField="fieldname" caseOf="true"/>
					</when>
				</dependentActions>
			</property>
			<property path="subCellDef.where" input="spark.components.TextInput" validator="whereClauseValidator"/>
			<property path="subCellDef.sort" input="spark.components.TextInput"/>
			<property path="subCellDef.limit" input="spark.components.NumericStepper" min="0" max="50" inc="1" init="0"/>
			<property path="subCellDef.height" dataType="Number" input="spark.components.NumericStepper" min="10" max="10000" init="50" copyable="true"/>
			<property path="subCellDef.Fields" input="LabelWithButton" popup="SubCellEditor" source="SubNestedList">
				<dependentActions>
					<when at="preCommit" stopCommit="true">
						<action target="{@form[subCellDef.Fields]:}" method="{@base:notifyChange()}"/>
					</when>
				</dependentActions>
			</property>
			<property path="subCellDef.backgroundColor" dataType="ColorDic" input="mx.controls.ColorPicker" copyable="true"/>
			<property path="subCellDef.backgroundColor.alpha" input="spark.components.NumericStepper" min="0.0" max="1.0" inc="0.1" copyable="true"/>
			<property path="subCellDef.borderColor" dataType="ColorDic" input="mx.controls.ColorPicker" disable="true"/>
			<property path="subCellDef.borderColor.alpha" input="spark.components.NumericStepper" min="0.0" max="1.0" inc="0.1" disable="true"/>
			<property path="subCellDef.borderWeight" dataType="Number" input="spark.components.NumericStepper" min="0" max="10" inc="1" init="0" disable="true"/>
			<property path="subCellDef.cornerRadius" dataType="Number" input="spark.components.NumericStepper" min="0" max="25" inc="1" init="0" disable="true"/>
		</type>	
		
		<type name="Padding" @stopCommit="true">
			<property path="padding.top" dataType="Number" input="spark.components.NumericStepper" min="0" max="100" inc="1" init="2" copyable="true"/>
			<property path="padding.left" dataType="Number" input="spark.components.NumericStepper" min="0" max="100" inc="1" init="2" copyable="true"/>
			<property path="padding.right" dataType="Number" input="spark.components.NumericStepper" min="0" max="100" inc="1" init="2" copyable="true"/>
			<property path="padding.bottom" dataType="Number" input="spark.components.NumericStepper" min="0" max="100" inc="1" init="2" copyable="true"/>
		</type>
		<type name="Geometry" @stopCommit="true">
			<property path="frame.x" dataType="Number" input="spark.components.NumericStepper" min="0" max="10000" copyable="true"/>
			<property path="frame.y" dataType="Number" input="spark.components.NumericStepper" min="0" max="10000" copyable="true"/>
			<property path="frame.width" dataType="Number" input="spark.components.NumericStepper" min="50" max="10000" init="320" copyable="true"/>
			<property path="frame.height" dataType="Number" input="spark.components.NumericStepper" min="10" max="10000" init="108" copyable="true"/>
		</type> 
	</properties>
</item>
