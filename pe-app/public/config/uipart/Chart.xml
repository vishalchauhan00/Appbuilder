						<action target="{@form[vaxisTextStyle.fontSize]:}" method="folded" value="false" caseOf="true"/>
						<action target="{@form[barWidth]:}" method="folded" value="false" caseOf="true"/>
						<action target="{@form[seriesType]:}" method="folded" value="true" caseOf="true"/>
						<action target="{@form[seriesField]:}" method="folded" value="true" caseOf="true"/>
						<action target="{@form[pointSize]:}" method="folded" value="true" caseOf="true"/>
						<action target="{@form[dataFields[*].pointShape]:}" method="folded" value="true" caseOf="true"/>
						<action target="{@form[dataFields[*].fillpoints]:}" method="folded" value="true" caseOf="true"/>
					</when>
				</dependentActions>
			</property>
			<property path="showCurve" dataType="Boolean" input="spark.components.CheckBox" value="false" copyable="true"/>
