		<type name="ChartOptions">
			<property path="gridlines" input="spark.components.DropDownList" init="Default" copyable="true">
				<dataSource>
					<data name="Default"/>
					<data name="None"/>			
				</dataSource>
			</property>
			<property path="chartTitle" dataType="String" input="spark.components.TextInput" copyable="true"/>
 			<property path="subTitle" dataType="String" input="spark.components.TextInput" disable="true"/>
			<property path="haxisTitle" dataType="String" input="spark.components.TextInput" copyable="true"/>
			<property path="haxisBaseline" dataType="Boolean" input="spark.components.CheckBox" value="true" copyable="true"/>
 			<property path="haxisTextStyle.fontName" input="spark.components.DropDownList" dataType="array" init="system" copyable="true">
				<dataSource source="{@base:getFontNames()}" />
			</property>
			<property path="haxisTextStyle.fontSize" dataType="Number" input="spark.components.NumericStepper" min="6" max="100" inc="1" init="12" copyable="true"/>
			<property path="vaxisTitle" dataType="String" input="spark.components.TextInput" copyable="true"/>
			<property path="vaxisBaseline" dataType="Boolean" input="spark.components.CheckBox" value="true" copyable="true"/>
			<property path="vaxisTextStyle.fontName" input="spark.components.DropDownList" dataType="array" init="system" copyable="true">
				<dataSource source="{@base:getFontNames()}" />
			</property>
			<property path="vaxisTextStyle.fontSize" dataType="Number" input="spark.components.NumericStepper" min="6" max="100" inc="1" init="12" copyable="true"/>
			<property path="chartarea.top" dataType="Number" input="spark.components.NumericStepper" min="0" max="1000" inc="1" init="0" copyable="true"/>
			<property path="chartarea.left" dataType="Number" input="spark.components.NumericStepper" min="0" max="1000" inc="1" init="0" copyable="true"/>
			<property path="chartarea.right" dataType="Number" input="spark.components.NumericStepper" min="0" max="1000" inc="1" init="0" copyable="true"/>
			<property path="chartarea.bottom" dataType="Number" input="spark.components.NumericStepper" min="0" max="1000" inc="1" init="0" copyable="true"/>
			<property path="chartarea.width" dataType="Number" input="spark.components.NumericStepper" min="0" max="100" inc="1" init="100" copyable="true"/>
			<property path="chartarea.height" dataType="Number" input="spark.components.NumericStepper" min="0" max="100" inc="1" init="100" copyable="true"/>
			<property path="showLegends" dataType="Boolean" input="spark.components.CheckBox" value="false" copyable="true">
				<dependentActions>
					<when condition="compare" compareTarget="{@form[showLegends]:value}" compareBy="EQ" compareWith="1" at="initForm, postCommit">
						<action target="{@form[legendPosition]:}" method="folded" value="false" caseOf="true"/>
						<action target="{@form[legendPosition]:}" method="folded" value="true" caseOf="false"/>
						<action target="{@form[legendTextStyle.fontName]:}" method="folded" value="false" caseOf="true"/>
						<action target="{@form[legendTextStyle.fontName]:}" method="folded" value="true" caseOf="false"/>
						<action target="{@form[legendTextStyle.fontSize]:}" method="folded" value="false" caseOf="true"/>
						<action target="{@form[legendTextStyle.fontSize]:}" method="folded" value="true" caseOf="false"/>
					</when>
				</dependentActions>
			</property>
			<property path="legendPosition" input="spark.components.DropDownList" init="right">
				<dataSource>
					<data name="top"/>
					<data name="bottom"/>
					<data name="left"/>
					<data name="right"/>				
				</dataSource>
			</property>
			<property path="legendTextStyle.fontName" input="spark.components.DropDownList" dataType="array" init="system" copyable="true">
				<dataSource source="{@base:getFontNames()}" />
			</property>
			<property path="legendTextStyle.fontSize" dataType="Number" input="spark.components.NumericStepper" min="6" max="100" inc="1" init="12" copyable="true">
			</property>
			<property path="barWidth" input="spark.components.NumericStepper" min="10" max="100" inc="1" copyable="true"/>
			<property path="pointSize" input="spark.components.NumericStepper" min="0" max="100" inc="1" copyable="true"/>			
		</type>
		
