					<data name="Bar"/>
					<data name="Column"/>
					<data name="Area"/>
					<data name="Pie"/>
					<data name="Donut"/>
					<data name="CandleStick"/>
					<data name="Combo"/>
					<data name="Bubble"/>	 				
					<data name="Scatter"/>
